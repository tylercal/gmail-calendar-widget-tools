{
  "manifest_version": 2,

  "name": "google calendar widget tools",
  "description": "this makes the calendar widget in gmail bigger",
  "version": "1.0.2",

  "icons": {
    "16": "images/calendar-16.png",
    "48": "images/calendar-48.png",
    "128": "images/calendar-128.png"},

  "content_scripts": [
    {
      "all_frames": true,
      "matches": ["https://mail.google.com/*", "https://calendar.google.com/calendar/*/mgadget*"],
      "js": ["script.js"]
    }
  ]
}